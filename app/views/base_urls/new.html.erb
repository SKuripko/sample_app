<% provide(:title, 'Add new API URL') %>
<div class="row">
	<div class="span6 offset3">
		<%= form_for(@api_url) do |f| %>
			<%= render 'shared/error_messages' %>

			<%= f.label :name %>
			<%= f.text_field :name %>

			<%= f.label :value %>
			<%= f.text_field :value %>

			<%= f.submit "Add new API URL", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>