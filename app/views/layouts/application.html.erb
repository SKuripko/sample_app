<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= include_gon %>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>      
    <script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>  
    <script type="text/javascript"></script>
    <script src="https://cdn.anychart.com/releases/v8/js/anychart-base.min.js" type="text/javascript"></script>
    <script src="https://cdn.anychart.com/releases/8.7.0/js/anychart-pie.min.js"></script>


    
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': true %>
    <%= javascript_include_tag 'application',               'data-turbolinks-track': true %>
    <%= javascript_include_tag 'bookings',                  'data-turbolinks-track': true %>
    <%= javascript_include_tag 'authorization_keys',        'data-turbolinks-track': true %>
    <%= javascript_include_tag 'search_results',            'data-turbolinks-track': true %>
    <%= javascript_include_tag 'bookings_info',             'data-turbolinks-track': true %>
    <%= javascript_include_tag 'cancel_bookings',           'data-turbolinks-track': true %>
    <%= javascript_include_tag 'fare_conditions',           'data-turbolinks-track': true %>
    <%= javascript_include_tag 'fare_families',             'data-turbolinks-track': true %>
    <%= javascript_include_tag 'flights_info',              'data-turbolinks-track': true %>
    <%= javascript_include_tag 'payment_bookings',          'data-turbolinks-track': true %>
    <%= javascript_include_tag 'payments_info',             'data-turbolinks-track': true %>
    <%= javascript_include_tag 'searchs',                   'data-turbolinks-track': true %>
    <%= javascript_include_tag 'passenger_generator',       'data-turbolinks-track': true %>
    <%= javascript_include_tag 'hits',                      'data-turbolinks-track': true %>
    <%= javascript_include_tag 'channels',                  'data-turbolinks-track': true %>
    <%= javascript_include_tag 'ace',                       'data-turbolinks-track': true %>            
    <%= render 'layouts/shim' %>
  </head>
  <body>
    <%= render 'layouts/header' %>  
    <div class="container-fluid">
      <% flash.each do |key, value| %>
        <%= content_tag(:div, value, class: "alert alert-#{key}") %>
      <% end %>  
      <%= yield %>
    </div>    
  </body>  
</html>
