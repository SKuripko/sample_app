<%= form_tag('/searchresult', method: :post, route: searchresult_path, onSubmit:'saveSearchResult()') do |f| %>
    <div class="search_result_fields">
      <div class="col-md-3">
        <label for="Select_API_URL">API</label>
        <%= select_tag(:api_url,options_from_collection_for_select(BaseUrl.all, :value, :name)) %>
      </div>      
      <div class="form-group search_result">
        <div class="col-md-6">
          <label for="validationDefault01" class="form-label">SRO</label>
          <input type="text" class="form-control form-control-sm" name="sro" id="sro" placeholder="SRO" required>
        </div>
        <div class="col-md-6">
          <label for="validationDefault02" class="form-label">Authorization key</label>
          <input type="text" class="form-control form-control-sm" name="auth_key" id="auth_key" placeholder="Authorization key" required>
        </div>          
      </div>
        <button type="submit" class="btn btn-primary btn-sm">Submit</button>
    </div>  
<% end %>